<Window x:Class="de.janhendrikpeters.helpdesk.toolkit.LogConsole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:de.janhendrikpeters.helpdesk.toolkit"
        mc:Ignorable="d"
        Title="LogConsole" MaxHeight="300" MaxWidth="300" Closing="Window_Closing">
    <DockPanel>
        <ScrollViewer Name="ScrollViewerMessages">
            <TextBox Name="TextBlockMessages" IsReadOnly="True"></TextBox>
        </ScrollViewer>
    </DockPanel>
</Window>
